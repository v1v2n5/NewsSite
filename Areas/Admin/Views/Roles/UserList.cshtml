@model IEnumerable<NewsSite.Models.User>
 
<h1>Список пользователей</h1>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.NickName</td>
            <td>@user.Email</td>

            <td>
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Права доступа</a>
            </td>
        </tr>
    }
</table>